{
  "name": "todo-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install && npx husky add .husky/pre-commit 'npx lint-staged'",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "start": "nodemon -e js,ejs",
    "start:prod": "node index.js",
    "pretest": "cross-env NODE_ENV=test npx sequelize-cli db:drop && cross-env NODE_ENV=test npx sequelize-cli db:create",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create"
  },
  "author": "Muhammed Shibil M <muhammedshibilm321@gmail.com --location=global>",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "cheerio": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^13.0.3",
    "nodemon": "^3.1.7",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.0",
    "pg": "^8.13.0",
    "sequelize": "^6.37.3",
    "tiny-csrf": "^1.1.4"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
